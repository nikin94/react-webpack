  "plugins": [
    "@loadable/babel-plugin",
    ["@babel/plugin-transform-runtime", { "regenerator": true }],
    ["@babel/plugin-transform-react-jsx", { "runtime": "automatic" }],
    "babel-plugin-webpack-alias"
  ]
}
